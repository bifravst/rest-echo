{
  "name": "@nordicsemiconductor/rest-echo",
  "version": "0.0.0-development",
  "description": "A REST echo server used for the Nordic Developer Academy developed using AWS CDK in TypeScript.",
  "type": "module",
  "scripts": {
    "prepare": "husky install && check-node-version --package",
    "test:e2e": "npx tsx --no-warnings ./features/run-features.ts | npx tsx --no-warnings ./features/console-reporter.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/rest-echo.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/rest-echo/issues"
  },
  "homepage": "https://github.com/bifravst/rest-echo#readme",
  "keywords": [
    "nordicsemiconductor",
    "nordic-developer-academy",
    "aws",
    "cdk",
    "rest"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.373.0",
    "@aws-sdk/client-cloudwatch-logs": "3.370.0",
    "@aws-sdk/client-dynamodb": "3.370.0",
    "@aws-sdk/client-iam": "3.370.0",
    "@aws-sdk/util-dynamodb": "3.370.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.60",
    "@nordicsemiconductor/bdd-markdown": "5.6.0",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "15.1.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.70",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "aws-cdk": "2.87.0",
    "aws-cdk-lib": "2.87.0",
    "cdk": "2.87.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "constructs": "10.2.69",
    "eslint-plugin-unicorn": "47.0.0",
    "husky": "8.0.3",
    "jsonata": "2.0.3",
    "prettier": "3.0.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@nordicsemiconductor/from-env": "2.0.0"
  }
}
